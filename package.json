{
  "name": "dumber",
  "version": "0.8.9",
  "description": "A dumb JavasScript bundler for Single Page Application, dumber than you and me",
  "main": "dist/index.js",
  "browser": {
    "./dist/cache/default.js": "./dist/cache/local-storage.js",
    "./dist/package-locators/default.js": "./dist/package-locators/jsDelivr.js",
    "./dist/log.js": "./dist/log-browser.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint src spec",
    "preversion": "npm test",
    "version": "standard-changelog && git add CHANGELOG.md",
    "postversion": "git push && git push --tags && npm publish",
    "pretest": "npm run lint",
    "prepare": "del-cli dist && babel src -d dist",
    "nodejs-test": "tape -r @babel/register \"spec/**/*.spec.js\" | tap-dot",
    "coverage": "nyc --reporter=lcov tape -r @babel/register \"spec/**/*.spec.js\" | tap-dot",
    "browser-test": "browserify -t babelify spec/all-browser-spec.js | tape-run | tap-dot",
    "test": "npm run nodejs-test && npm run browser-test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dumberjs/dumber"
  },
  "keywords": [
    "bundle",
    "browser",
    "spa",
    "amd"
  ],
  "author": "Chunpeng Huo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dumberjs/dumber/issues"
  },
  "homepage": "https://github.com/dumberjs/dumber#readme",
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "ast-matcher": "^1.0.2",
    "base64-arraybuffer": "^0.1.5",
    "cherow": "^1.6.9",
    "convert-source-map": "^1.6.0",
    "del": "^3.0.0",
    "dumber-module-loader": "^0.15.13",
    "escope": "^3.6.0",
    "mkdirp": "^0.5.1",
    "node-fetch": "^2.3.0",
    "node-libs-browser": "^2.1.0",
    "readable-stream": "^2.3.6",
    "resolve": "^1.9.0",
    "source-map": "^0.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "del-cli": "^1.1.0",
    "eslint": "^5.12.0",
    "nyc": "^13.1.0",
    "standard-changelog": "^2.0.6",
    "tap-dot": "^2.0.0",
    "tape": "^4.9.2",
    "tape-run": "^5.0.0"
  }
}
