{
  "name": "dumber",
  "version": "0.14.0",
  "description": "A dumb JavasScript bundler for Single Page Application, dumber than you and me",
  "main": "index.js",
  "files": [
    "index.js",
    "lib"
  ],
  "browser": {
    "./lib/cache/default.js": "./lib/cache/local-storage.js",
    "./lib/package-file-reader/default.js": "./lib/package-file-reader/jsDelivr.js",
    "./lib/log.js": "./lib/log-browser.js"
  },
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "lint": "eslint index.js lib test",
    "preversion": "npm test",
    "version": "standard-changelog && git add CHANGELOG.md",
    "postversion": "git push && git push --tags && npm publish",
    "pretest": "npm run lint",
    "nodejs-test": "tape test/hide-log.js \"test/**/*.spec.js\" | tap-dot",
    "coverage": "nyc --reporter=lcov tape test/hide-log.js \"test/**/*.spec.js\" | tap-dot",
    "browser-test": "browserify test/all-browser-spec.js | browser-do --tap | tap-dot",
    "test": "npm run nodejs-test && npm run browser-test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dumberjs/dumber"
  },
  "keywords": [
    "bundle",
    "browser",
    "spa",
    "amd"
  ],
  "author": "Chunpeng Huo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dumberjs/dumber/issues"
  },
  "homepage": "https://github.com/dumberjs/dumber#readme",
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "ast-matcher": "^1.0.4",
    "base64-arraybuffer": "^0.2.0",
    "cherow": "^1.6.9",
    "convert-source-map": "^1.6.0",
    "del": "^5.0.0",
    "dumber-module-loader": "^0.18.0",
    "escope": "^3.6.0",
    "mkdirp": "^0.5.1",
    "modify-code": "^0.5.3",
    "node-fetch": "^2.6.0",
    "node-libs-browser": "^2.2.1",
    "readable-stream": "^2.3.6",
    "resolve": "^1.11.1",
    "source-map": "^0.7.3"
  },
  "devDependencies": {
    "browserify": "^16.3.0",
    "eslint": "^6.1.0",
    "nyc": "^14.1.1",
    "sourcemap-codec": "^1.4.6",
    "standard-changelog": "^2.0.13",
    "tap-dot": "^2.0.0",
    "tape": "^4.11.0",
    "browser-do": "^0.3.2"
  }
}
