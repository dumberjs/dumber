{
  "name": "dumber",
  "version": "0.2.1",
  "description": "A dumb bundler for building JavasScript Single Page Application, dumber than you and me",
  "main": "dist/index.js",
  "browser": {
    "./cache/default.js": "./cache/local-storage.js",
    "./package-locators/default.js": "./package-locators/jsDelivr.js"
  },
  "types": "index.d.ts",
  "files": [
    "index.d.ts",
    "dist"
  ],
  "scripts": {
    "lint": "eslint src spec",
    "preversion": "npm test",
    "version": "standard-changelog && git add CHANGELOG.md",
    "postversion": "git push && git push --tags",
    "pretest": "npm run lint",
    "prepare": "babel src -d dist",
    "nodejs-test": "tape -r @babel/register 'spec/**/*.spec.js' | tap-dot",
    "coverage": "nyc --reporter=lcov tape -r @babel/register 'spec/**/*.spec.js' | tap-dot",
    "browser-test": "browserify -t babelify spec/all-browser-spec.js | tape-run | tap-dot",
    "test": "npm run nodejs-test && npm run browser-test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huochunpeng/dumber.git"
  },
  "keywords": [
    "bundle",
    "browser",
    "amd"
  ],
  "author": "Chunpeng Huo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/huochunpeng/dumber/issues"
  },
  "homepage": "https://github.com/huochunpeng/dumber#readme",
  "dependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-transform-modules-amd": "^7.1.0",
    "ast-matcher": "^1.0.1",
    "del": "^3.0.0",
    "dumber-module-loader": "bitbucket:huochunpeng/dumber-module-loader#v0.10.2",
    "escope": "^3.6.0",
    "esprima": "^4.0.1",
    "mkdirp": "^0.5.1",
    "node-fetch": "^2.3.0",
    "node-libs-browser": "^2.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/preset-env": "^7.1.6",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "eslint": "^5.9.0",
    "nyc": "^13.1.0",
    "standard-changelog": "^2.0.6",
    "tap-dot": "^2.0.0",
    "tape": "^4.9.1",
    "tape-run": "^4.0.0"
  }
}
