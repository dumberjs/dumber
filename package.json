{
  "name": "dumber",
  "version": "1.14.12",
  "description": "A dumb JavasScript bundler for Single Page Application, dumber than you and me",
  "main": "index.js",
  "files": [
    "index.js",
    "lib"
  ],
  "browser": {
    "./lib/cache/default.js": "./lib/cache/none.js",
    "./lib/package-file-reader/default.js": "./lib/package-file-reader/jsdelivr.js",
    "./lib/log.js": "./lib/log-browser.js",
    "./lib/resolve-package.js": "./lib/resolve-package-browser.js"
  },
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "lint": "eslint index.js lib test",
    "preversion": "npm test",
    "version": "standard-changelog && git add CHANGELOG.md",
    "postversion": "git push && git push --tags && npm publish",
    "pretest": "npm run lint",
    "nodejs-test": "tape test/hide-log.js \"test/**/*.spec.js\"",
    "coverage": "nyc --reporter=lcov tape test/hide-log.js \"test/**/*.spec.js\"",
    "browser-test": "browserify test/all-browser-spec.js | browser-do --tap",
    "test": "npm run nodejs-test && npm run browser-test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dumberjs/dumber"
  },
  "keywords": [
    "bundle",
    "browser",
    "spa",
    "amd"
  ],
  "author": "Chunpeng Huo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dumberjs/dumber/issues"
  },
  "homepage": "https://github.com/dumberjs/dumber#readme",
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.16.0",
    "assert": "^2.0.0",
    "ast-matcher": "^1.1.1",
    "base64-arraybuffer": "^1.0.1",
    "browserify-zlib": "^0.2.0",
    "buffer": "^5.7.0",
    "console-browserify": "^1.2.0",
    "constants-browserify": "^1.0.0",
    "convert-source-map": "^1.8.0",
    "crypto-browserify": "^3.12.0",
    "del": "^5.1.0",
    "domain-browser": "^4.22.0",
    "dumber-module-loader": "^1.2.0",
    "eslint-scope": "^7.1.0",
    "events": "^3.3.0",
    "fs-browser-stub": "^1.0.1",
    "https-browserify": "^1.0.0",
    "meriyah": "^4.2.0",
    "mkdirp-classic": "^0.5.3",
    "modify-code": "^2.0.0",
    "node-fetch": "^2.6.6",
    "os-browserify": "^0.3.0",
    "path-browserify": "1.0.1",
    "process": "^0.11.10",
    "punycode": "^2.1.1",
    "querystring-browser-stub": "^1.0.0",
    "readable-stream": "^3.6.0",
    "resolve": "1.19.0",
    "source-map": "^0.8.0-beta.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "string_decoder": "^1.3.0",
    "timers-browserify": "^2.0.12",
    "tty-browserify": "0.0.1",
    "url": "^0.11.0",
    "util": "^0.12.4",
    "vm-browserify": "^1.1.2"
  },
  "devDependencies": {
    "browser-do": "^4.0.0",
    "browserify": "^17.0.0",
    "eslint": "^8.3.0",
    "nyc": "^15.1.0",
    "semver": "^7.3.5",
    "sourcemap-codec": "^1.4.8",
    "standard-changelog": "^2.0.27",
    "tape": "^5.3.2"
  }
}
