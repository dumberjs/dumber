{
  "name": "dumber",
  "version": "0.12.0",
  "description": "A dumb JavasScript bundler for Single Page Application, dumber than you and me",
  "main": "dist/index.js",
  "browser": {
    "./dist/cache/default.js": "./dist/cache/local-storage.js",
    "./dist/package-file-reader/default.js": "./dist/package-file-reader/jsDelivr.js",
    "./dist/log.js": "./dist/log-browser.js"
  },
  "engines": {
    "node": ">=8.9"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint src test",
    "preversion": "npm test",
    "version": "standard-changelog && git add CHANGELOG.md",
    "postversion": "git push && git push --tags && npm publish",
    "pretest": "npm run lint",
    "prepare": "del-cli dist && babel src -d dist",
    "nodejs-test": "tape -r @babel/register test/hide-log.js \"test/**/*.spec.js\" | tap-dot",
    "coverage": "nyc --reporter=lcov tape -r @babel/register test/hide-log.js \"test/**/*.spec.js\" | tap-dot",
    "browser-test": "browserify -t babelify test/all-browser-spec.js | browser-do --tap | tap-dot",
    "test": "npm run nodejs-test && npm run browser-test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dumberjs/dumber"
  },
  "keywords": [
    "bundle",
    "browser",
    "spa",
    "amd"
  ],
  "author": "Chunpeng Huo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dumberjs/dumber/issues"
  },
  "homepage": "https://github.com/dumberjs/dumber#readme",
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "ast-matcher": "^1.0.2",
    "base64-arraybuffer": "^0.2.0",
    "cherow": "^1.6.9",
    "convert-source-map": "^1.6.0",
    "del": "^4.1.1",
    "dumber-module-loader": "^0.17.0",
    "escope": "^3.6.0",
    "mkdirp": "^0.5.1",
    "modify-code": "^0.2.1",
    "node-fetch": "^2.6.0",
    "node-libs-browser": "^2.2.1",
    "readable-stream": "^2.3.6",
    "resolve": "^1.11.1",
    "source-map": "^0.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.2",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "del-cli": "^2.0.0",
    "eslint": "^5.16.0",
    "nyc": "^14.1.1",
    "sourcemap-codec": "^1.4.4",
    "standard-changelog": "^2.0.11",
    "tap-dot": "^2.0.0",
    "tape": "^4.10.2",
    "browser-do": "^0.2.0"
  }
}
